# Configuration du serveur
PORT=3000
BASE_URL=http://localhost:3000
BASE_DIR=
MAINTENANCE_MODE=0 # 0 = désactivé, 1 = activé

# Configuration MySQL
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=your_username
MYSQL_PASSWORD=your_secure_password
MYSQL_DB=your_database_name

# Clés de chiffrement - Générer des clés sécurisées en production
MASTER_KEY_V1=your_64_character_hex_master_key_here_replace_with_actual_key
KEY_VERSION=1

# Limitation de taux (Rate limiting)
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=100

# Niveau de log (debug, info, warn, error, fatal)
LOG_LEVEL=info

# Configuration des sessions
SESSION_SECRET=your_64_character_session_secret_replace_with_random_string
SESSION_COOKIE_NAME=sessionid
SESSION_TTL_SECONDS=86400
